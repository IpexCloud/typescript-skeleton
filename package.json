{
  "name": "typescript-skeleton",
  "author": "Ipex dev-team",
  "version": "0.0.0",
  "description": "TypeScript quick start skeleton",
  "scripts": {
    "build": "rimraf build && tsc -p tsconfig.json && ttsc",
    "postinstall": "rimraf build && tsc -p tsconfig.json && ttsc",
    "build:watch": "nodemon --watch src --watch config --exec \"ts-node -r tsconfig-paths/register\" src/main.ts",
    "start": "node build/src/main",
    "typeorm": "ts-node -r ./tsconfig.json ./node_modules/typeorm/cli.js",
    "test": "jest",
    "test:watch": "nodemon -e ts,json --watch src --watch config --watch tests --exec jest -- --collectCoverage false"
  },
  "license": "MIT",
  "dependencies": {
    "@godaddy/terminus": "^4.7.1",
    "apollo-server-express": "^2.19.2",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "class-validator-jsonschema": "^2.2.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-winston": "^4.1.0",
    "graphql": "^14.7.0",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "routing-controllers": "^0.8.1",
    "routing-controllers-openapi": "^2.2.0",
    "swagger-ui-express": "^4.1.6",
    "tsconfig-paths-jest": "0.0.1",
    "tslib": "~1.10.0",
    "type-graphql": "^0.17.6",
    "typeorm": "^0.2.32",
    "uuid": "^3.4.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.8",
    "@types/jest": "^25.2.3",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/multer": "^1.4.5",
    "@types/node": "^13.13.51",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^3.4.9",
    "@types/ws": "^7.4.2",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^25.5.4",
    "nodemon": "^2.0.7",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.5",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "tsutils": "~3.17.0",
    "ttypescript": "^1.5.12",
    "typescript": "^3.9.9"
  }
}
