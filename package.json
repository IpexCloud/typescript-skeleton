{
  "name": "node-typescript-boilerplate",
  "author": "Ipex dev-team",
  "version": "0.0.0",
  "description": "TypeScript quick start skeleton",
  "scripts": {
    "build": "rimraf build && tsc -p tsconfig.json && ttsc",
    "build:watch": "nodemon --watch src --watch config --exec \"ts-node -r tsconfig-paths/register\" src/main.ts",
    "start": "node build/src/main",
    "typeorm": "ts-node -r ./tsconfig.json ./node_modules/typeorm/cli.js",
    "test": "jest",
    "test:watch": "nodemon -e ts,json --watch src --watch config --exec jest -- --collectCoverage false"
  },
  "license": "MIT",
  "dependencies": {
    "@godaddy/terminus": "^4.3.1",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "class-validator-jsonschema": "^1.3.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "express-winston": "^4.0.3",
    "graphql": "^14.6.0",
    "graphql-playground-middleware-express": "^1.7.12",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "routing-controllers": "^0.8.0",
    "routing-controllers-openapi": "^1.8.0",
    "swagger-ui-express": "^4.1.3",
    "tsconfig-paths-jest": "0.0.1",
    "tslib": "~1.10.0",
    "type-graphql": "^0.17.6",
    "typeorm": "^0.2.22",
    "uuid": "^3.4.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/jest": "^25.1.3",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/multer": "^1.4.0",
    "@types/node": "^13.5.3",
    "@types/swagger-ui-express": "^4.1.1",
    "@types/uuid": "^3.4.7",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "@zerollup/ts-transform-paths": "^1.7.12",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-jest": "^23.8.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.1",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "tsutils": "~3.17.0",
    "ttypescript": "^1.5.10",
    "typescript": "^3.8.2"
  }
}
